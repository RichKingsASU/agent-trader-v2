# Cloud Run env-vars file (YAML) for execution engine service.
#
# Copy to a local, untracked file:
#   cp infra/cloudrun/env/execution_engine.env.yaml.example ./execution_engine.env.yaml
# Then fill values and pass to deploy script via:
#   ENV_VARS_FILE=./execution_engine.env.yaml ./infra/cloudrun/deploy_execution_engine.sh
#
# IMPORTANT: do not commit real values.

# Firebase / Firestore (ADC uses attached service account)
FIREBASE_PROJECT_ID: ""

# Vertex AI (Gemini)
VERTEX_AI_MODEL_ID: "gemini-2.5-flash"
VERTEX_AI_LOCATION: "us-central1"
# Prefer mapping Vertex project to your Firebase project id.
VERTEX_AI_PROJECT_ID: ""

# Broker (Alpaca) credentials (RECOMMENDED to inject via Secret Manager)
ALPACA_API_KEY: ""
ALPACA_SECRET_KEY: ""
ALPACA_TRADING_HOST: ""  # default is paper; set to live intentionally

# Execution behavior
EXEC_DRY_RUN: ""         # "1" (recommended by default) or "0"

# Risk rules (optional)
EXEC_MAX_POSITION_QTY: ""
EXEC_MAX_DAILY_TRADES: ""
EXEC_KILL_SWITCH: ""
EXEC_KILL_SWITCH_ENV: ""
EXEC_KILL_SWITCH_DOC: ""
EXEC_RISK_FAIL_OPEN: ""

# Optional Postgres source for positions
DATABASE_URL: ""

LOG_LEVEL: ""


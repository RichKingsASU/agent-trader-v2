# Cloud Run env-vars file (YAML) for the optional backfill job.
#
# Copy to a local, untracked file:
#   cp infra/cloudrun/env/backfill_bars.env.yaml.example ./backfill_bars.env.yaml
#
# IMPORTANT: do not commit real values.

# Alpaca credentials
# Canonical (official Alpaca SDK env vars):
APCA_API_KEY_ID: ""
APCA_API_SECRET_KEY: ""
APCA_API_BASE_URL: "https://paper-api.alpaca.markets"
#
# Back-compat aliases (still accepted by repo env normalization; prefer APCA_* above):
# ALPACA_API_KEY: ""
# ALPACA_SECRET_KEY: ""

# Vertex AI (Gemini) (optional)
VERTEX_AI_MODEL_ID: "gemini-2.5-flash"
VERTEX_AI_LOCATION: "us-central1"
VERTEX_AI_PROJECT_ID: ""

# Backfill config
ALPACA_FEED: ""          # e.g. "iex"
ALPACA_SYMBOLS: ""       # e.g. "SPY,IWM"
ALPACA_BACKFILL_DAYS: "" # e.g. "5"

# Target database (required)
DATABASE_URL: ""

LOG_LEVEL: ""

# Repo-wide safety hard lock (required by backend/common/agent_mode_guard.py)
AGENT_MODE: "OFF"        # OFF|OBSERVE|EVAL|PAPER (EXECUTE is forbidden)
TRADING_MODE: "paper"    # must be exactly "paper"

